# 1. 저장소 초기화 (이미 GitHub에서 클론한 경우 생략)
git init

# 2. 파일 생성 및 커밋
echo "Hello, world!" > file1.txt
git add file1.txt
git commit -m "A"

# 3. 새 브랜치 만들기 및 작업
git checkout -b new-branch
echo "This is a change in new-branch" > file2.txt
git add file2.txt
git commit -m "B"

# 4. main 브랜치로 돌아와서 병합
git checkout main
git merge new-branch
git commit -m "C"

# 5. 또 다른 브랜치 작업
git checkout -b another-branch
echo "This is a change in another-branch" > file3.txt
git add file3.txt
git commit -m "D"

# 6. main 브랜치로 돌아와서 병합
git checkout main
git merge another-branch
git commit -m "E"

# 7. 변경사항을 GitHub에 푸시
git push origin main
git push origin new-branch
git push origin another-branch
